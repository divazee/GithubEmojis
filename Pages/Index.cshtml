@page
@model IndexModel
@{
    //ViewData["Title"] = "Home page";
}

<h1>Github Emoji</h1>

<div class="allEmojis">
    @foreach (var em in Model.Emojis)
    {
        <div class="emoji">
            <img src="@em.Url" alt="Emoji - @em.Key" height="128" width="128" />
            <code>@em.Key</code>
        </div>
    }

</div>




